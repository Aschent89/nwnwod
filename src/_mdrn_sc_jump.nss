int StartingConditional()
{

    // Inspect local variables
    if(!(GetLocalInt(GetPCSpeaker(), "JUMP_INVALID") == 0) && GetIsDM(GetPCSpeaker())==FALSE)
        return FALSE;

    return TRUE;
}
